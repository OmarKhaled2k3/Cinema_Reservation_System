            </StackPane>
            <HBox layoutX="16.0" layoutY="316.0" prefHeight="83.0" prefWidth="584.0" spacing="70.0">
               <children>
                  <Button fx:id="back_button" layoutX="186.0" layoutY="10.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="88.0" text="back" />
                  <Button fx:id="add_button" mnemonicParsing="false" prefHeight="42.0" prefWidth="88.0" text="Add" />
                  <Button fx:id="remove_button" mnemonicParsing="false" prefHeight="42.0" prefWidth="88.0" text="Remove" />
                  <Button fx:id="save_button" mnemonicParsing="false" prefHeight="42.0" prefWidth="88.0" text="Save" />
               </children>
            </HBox>
            <StackPane layoutX="235.0" layoutY="14.0" prefHeight="51.0" prefWidth="131.0">
               <children>
                  <Label fx:id="movie_title" text="Movie">
                     <font>
                        <Font size="28.0" />
                     </font>
                  </Label>
               </children>
            </StackPane>
            <VBox fx:id="iamgeuellabel" layoutX="14.0" layoutY="81.0" prefHeight="129.0" prefWidth="104.0" spacing="6.0">
