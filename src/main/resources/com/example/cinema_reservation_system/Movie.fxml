                  <Label fx:id="duration1" prefHeight="26.0" prefWidth="47.0" text="Duration" />
                  <Label prefHeight="26.0" prefWidth="50.0" text="ImageUrl" />
               </children>
            </VBox>
            <VBox layoutX="95.0" layoutY="81.0" spacing="6.0">
               <children>
                  <TextField fx:id="namebox" />
                  <TextField fx:id="genrebox" />
                  <TextField fx:id="durationbox" />
                  <TextField fx:id="imageurlbox" />
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</StackPane>
